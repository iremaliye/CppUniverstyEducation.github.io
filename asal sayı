#include <iostream>
using namespace std;
bool asalmi(int x) {
	for (int i = 2; i < x; i++) {
		if (x % i == 0) {
			return false;
		}
	}
	return true;
}
int main()
//asal sayıları yazdıran kod
{
	int c = 0;
	int i
	//en küçük asal sayı 2 dir.
	for (int(i = 2; i < 1000; i++){
		//1000 e kadar git asalsa bas
		if (asalmi(i)) {
			cout<< i <<endl;
				c++;
		}
     }
}

